{% extends "base.twig" %}
{% block content %}
	<div class="col-12">
		<div class="row">
			<div class="container">
				<div class="content-wrapper">
					<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
						<section class="article-content">
							<h1 class="article-h1">{{ post.title }}w</h1>
							<div class="article-body">
                                {{post.content}}
								<img src="{{ placholder }}?water/800x800" alt="" class="img-fluid">
							</div>
						</section>
					</article>
				</div><!-- /content-wrapper re-->
			</div>
		</div>
	</div>
{% endblock %}
